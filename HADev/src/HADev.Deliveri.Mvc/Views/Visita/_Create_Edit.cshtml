@model HADev.Delivery.Domain.Models.Visita

<div class="form-group">
    <label asp-for="EleitorId" class="control-label"></label>
    <select asp-for="EleitorId" class="form-control" asp-items="ViewBag.EleitorId"></select>
</div>
<div class="form-group">
    <label asp-for="DataVisita" class="control-label"></label>
    <input asp-for="DataVisita" class="form-control" />
    <span asp-validation-for="DataVisita" class="text-danger"></span>
</div>
<div class="form-group">
    <label asp-for="Obs" class="control-label"></label>
    <textarea asp-for="Obs" class="form-control"></textarea>
    <span asp-validation-for="Obs" class="text-danger"></span>
</div>

<div class="form-group">
    <input type="submit" value="Salvar" class="btn btn-primary" />&nbsp;&nbsp;&nbsp;
    <a asp-controller="Visita" asp-action="Index" class="btn btn-default">Voltar</a>
</div>
